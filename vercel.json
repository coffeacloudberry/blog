{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-Robots-Tag",
                    "value": "none"
                }
            ]
        },
        {
            "source": "/",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'none'; img-src 'self'; style-src 'self'; font-src 'self'; frame-ancestors 'none'"
                },
                {
                    "key": "Permissions-Policy",
                    "value": "geolocation=(), camera=(), microphone=()"
                },
                {
                    "key": "Referrer-Policy",
                    "value": "strict-origin"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "https://blog.explorewilder.com"
                }
            ]
        }
    ]
}
